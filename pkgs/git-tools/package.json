{
	"name": "@duncan3142/git-tools",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Duncan3142/mono.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "restricted"
	},
	"description": "Git tools",
	"keywords": [],
	"version": "0.1.2",
	"author": "Duncan Giles",
	"type": "module",
	"license": "LGPL-3.0-or-later",
	"engines": {
		"node": ">=22",
		"npm": ">=11"
	},
	"main": "./.tsc/dist/index.js",
	"imports": {
		"#duncan3142/git-tools": "./.tsc/dist/index.js",
		"#duncan3142/git-tools/internal": "./.tsc/dist/internal/index.js",
		"#duncan3142/git-tools/core/command": "./.tsc/dist/core/command/index.js",
		"#duncan3142/git-tools/core/config": "./.tsc/dist/core/config/index.js",
		"#duncan3142/git-tools/core/context": "./.tsc/dist/core/context/index.js",
		"#duncan3142/git-tools/core/domain": "./.tsc/dist/core/domain/index.js",
		"#duncan3142/git-tools/core/executor": "./.tsc/dist/core/executor/index.js",
		"#duncan3142/git-tools/core/state": "./.tsc/dist/core/state/index.js",
		"#duncan3142/git-tools/core/telemetry": "./.tsc/dist/core/telemetry/index.js",
		"#duncan3142/git-tools/core": "./.tsc/dist/core/index.js",
		"#duncan3142/git-tools/git": "./.tsc/dist/git/index.js"
	},
	"exports": {
		".": {
			"import": "./.tsc/dist/index.js",
			"types": "./.tsc/dist/index.d.ts"
		},
		"./core": {
			"import": "./.tsc/dist/core/index.js",
			"types": "./.tsc/dist/core/index.d.ts"
		},
		"./git": {
			"import": "./.tsc/dist/git/index.js",
			"types": "./.tsc/dist/git/index.d.ts"
		}
	},
	"files": [
		".tsc/dist/**/*.js",
		".tsc/dist/**/*.ts",
		".tsc/dist/**/*.map"
	],
	"scripts": {
		"clean": "rm -rf .prettier .package .tsc",
		"format": "prettier --check --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"format:fix": "prettier --write --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"assets": "tsc --build",
		"assets:watch": "tsc --build --watch",
		"check:tsc": "tsc --build --noEmit",
		"lint:eslint:all": "TIMING=1 LINT_LEVEL=all eslint --max-warnings 0 .",
		"lint:eslint": "TIMING=1 eslint --max-warnings 0 .",
		"lint:eslint:fix": "LINT_LEVEL=all eslint --fix .",
		"test:vitest": "vitest run",
		"inspect:eslint": "LINT_LEVEL=all eslint --inspect-config ."
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@duncan3142/eslint-config": "^0.1.19",
		"@duncan3142/prettier-config": "^0.1.9",
		"@duncan3142/tsc-config": "^0.1.12",
		"@effect/language-service": "^0.44.0",
		"@effect/platform-node": "^0.98.3",
		"@effect/vitest": "^0.26.0",
		"@eslint/config-inspector": "^1.3.0",
		"@types/node": "^22.18.10",
		"eslint": "^9.37.0",
		"prettier": "^3.6.2",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"vitest-mock-extended": "^3.1.0"
	},
	"volta": {
		"node": "22.20.0",
		"npm": "11.6.2"
	},
	"dependencies": {
		"@duncan3142/effect": "^0.1.3",
		"@effect/platform": "^0.92.1",
		"effect": "^3.18.4"
	}
}
