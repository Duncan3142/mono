{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#const": ["./src/const.ts"],
			"#fetch/*": ["./src/lib/fetch/*.ts"],
			"#reference/*": ["./src/lib/reference/*.ts"],
			"#reference-spec/*": ["./src/lib/reference-spec/*.ts"],
			"#remote/*": ["./src/lib/remote/*.ts"]
		},
		"isolatedDeclarations": false
	},
	"include": []
}
