{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#const": ["./src/const.ts"],
			"#layer": ["./src/layer.ts"],
			"#service": ["./src/service.ts"],
			"#config/*": ["./src/lib/core/config/*.ts"],
			"#case/*": ["./src/lib/core/case/*.ts"],
			"#state/*": ["./src/lib/core/state/*.ts"],
			"#command/*": ["./src/lib/core/command/*.ts"],
			"#domain/*": ["./src/lib/core/domain/*.ts"],
			"#git/*": ["./src/lib/git/*.ts"],
			"#mock/*": ["./test/mock/*.ts"]
		},
		"isolatedDeclarations": false,
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	},
	"include": []
}
