{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#config/*": ["./src/config/*.ts"],
			"#error/*": ["./src/error/*.ts"],
			"#fetch/*": ["./src/fetch/*.ts"],
			"#reference/*": ["./src/reference/*.ts"],
			"#reference-spec/*": ["./src/reference-spec/*.ts"],
			"#remote/*": ["./src/remote/*.ts"]
		}
	},
	"include": []
}
