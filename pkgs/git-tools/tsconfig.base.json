{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#duncan3142/git-tools/layer": ["./src/layer.ts"],
			"#duncan3142/git-tools/command": ["./src/lib/core/command/index.ts"],
			"#duncan3142/git-tools/config": ["./src/lib/core/config/index.ts"],
			"#duncan3142/git-tools/const": ["./src/lib/core/const/index.ts"],
			"#duncan3142/git-tools/context": ["./src/lib/core/context/index.ts"],
			"#duncan3142/git-tools/domain": ["./src/lib/core/domain/index.ts"],
			"#duncan3142/git-tools/executor": ["./src/lib/core/executor/index.ts"],
			"#duncan3142/git-tools/metric": ["./src/lib/core/metric/index.ts"],
			"#duncan3142/git-tools/state": ["./src/lib/core/state/index.ts"],
			"#duncan3142/git-tools/core": ["./src/lib/core/index.ts"],
			"#duncan3142/git-tools/git": ["./src/lib/git/index.ts"],
			"#duncan3142/git-tools/test-mock": ["./test/mock/index.ts"],
			"#duncan3142/git-tools/test-setup": ["./test/setup/index.ts"]
		},
		"isolatedDeclarations": false,
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	},
	"include": []
}
