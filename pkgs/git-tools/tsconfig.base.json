{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#duncan3142/git-tools": ["./src/index.ts"],
			"#duncan3142/git-tools/internal": ["./src/internal/index.ts"],
			"#duncan3142/git-tools/core/command": ["./src/core/command/index.ts"],
			"#duncan3142/git-tools/core/config": ["./src/core/config/index.ts"],
			"#duncan3142/git-tools/core/context": ["./src/core/context/index.ts"],
			"#duncan3142/git-tools/core/domain": ["./src/core/domain/index.ts"],
			"#duncan3142/git-tools/core/executor": ["./src/core/executor/index.ts"],
			"#duncan3142/git-tools/core/state": ["./src/core/state/index.ts"],
			"#duncan3142/git-tools/core/telemetry": ["./src/core/telemetry/index.ts"],
			"#duncan3142/git-tools/core": ["./src/core/index.ts"],
			"#duncan3142/git-tools/git": ["./src/git/index.ts"]
		},
		"isolatedDeclarations": false,
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	},
	"include": []
}
