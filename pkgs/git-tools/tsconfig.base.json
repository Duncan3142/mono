{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@duncan3142/tsc-config/base"],
	"compilerOptions": {
		"paths": {
			"#duncan3142/git-tools": ["./src/index.ts"],
			"#duncan3142/git-tools/lib/core/command": ["./src/lib/core/command/index.ts"],
			"#duncan3142/git-tools/lib/core/config": ["./src/lib/core/config/index.ts"],
			"#duncan3142/git-tools/lib/core/context": ["./src/lib/core/context/index.ts"],
			"#duncan3142/git-tools/lib/core/domain": ["./src/lib/core/domain/index.ts"],
			"#duncan3142/git-tools/lib/core/executor": ["./src/lib/core/executor/index.ts"],
			"#duncan3142/git-tools/lib/core/state": ["./src/lib/core/state/index.ts"],
			"#duncan3142/git-tools/lib/core": ["./src/lib/core/index.ts"],
			"#duncan3142/git-tools/lib/git": ["./src/lib/git/index.ts"],
			"#duncan3142/git-tools/test/setup": ["./test/setup/index.ts"]
		},
		"isolatedDeclarations": false,
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	},
	"include": []
}
