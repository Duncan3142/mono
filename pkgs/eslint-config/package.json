{
	"name": "@duncan3142/eslint-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Duncan3142/mono.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "restricted"
	},
	"description": "ESLint config",
	"keywords": [],
	"version": "0.1.10",
	"author": "Duncan Giles",
	"type": "module",
	"license": "LGPL-3.0-or-later",
	"engines": {
		"node": ">=22",
		"npm": ">=11"
	},
	"imports": {
		"#lib/typescript": "./.tsc/dist/lib/typescript/index.js",
		"#lib/*": "./.tsc/dist/lib/*.js"
	},
	"exports": {
		"./typescript": {
			"import": "./.tsc/dist/lib/typescript/index.js",
			"types": "./.tsc/dist/lib/typescript/index.d.ts"
		},
		"./*": {
			"import": "./.tsc/dist/lib/*.js",
			"types": "./.tsc/dist/lib/*.d.ts"
		}
	},
	"files": [
		".tsc/dist/**/*.js",
		".tsc/dist/**/*.ts",
		".tsc/dist/**/*.map"
	],
	"scripts": {
		"clean": "rm -rf .prettier .package .tsc",
		"format": "prettier --check --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"format:fix": "prettier --write --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"assets": "tsc --build",
		"assets:watch": "tsc --build --watch",
		"check:tsc": "tsc --build --noEmit",
		"lint:eslint": "TIMING=1 LINT_LEVEL=all eslint --max-warnings 0 .",
		"lint:eslint:fix": "TIMING=1 LINT_LEVEL=all eslint --fix .",
		"inspect:eslint": "eslint --inspect-config ."
	},
	"peerDependencies": {
		"@types/eslint": "^9.6.1",
		"eslint": "^9.17.0"
	},
	"dependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.17.0",
		"arktype": "^2.1.20",
		"eslint-config-prettier": "^10.1.2",
		"eslint-import-resolver-typescript": "^4.3.4",
		"eslint-plugin-functional": "^9.0.1",
		"eslint-plugin-import-x": "^4.11.0",
		"eslint-plugin-jsdoc": "^50.6.11",
		"eslint-plugin-no-secrets": "^2.2.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-unicorn": "^59.0.0",
		"remeda": "^2.21.3",
		"typescript-eslint": "^8.31.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.2",
		"@duncan3142/prettier-config": "^0.1.3",
		"@duncan3142/tsc-config": "^0.1.6",
		"@eslint/config-inspector": "^1.0.2",
		"@types/eslint": "^9.6.1",
		"@types/eslint-config-prettier": "^6.11.3",
		"eslint": "^9.26.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3"
	},
	"volta": {
		"node": "22.13.1",
		"npm": "11.1.0"
	}
}
