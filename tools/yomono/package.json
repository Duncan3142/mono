{
	"name": "@duncan3142/yomono",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Duncan3142/mono.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com",
		"access": "restricted"
	},
	"description": "YoMono",
	"keywords": [],
	"version": "0.1.10",
	"author": "Duncan Giles",
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": ">=22",
		"npm": ">=11"
	},
	"main": "./.tsc/dist/core.js",
	"imports": {
		"#*": "./.tsc/dist/*.js"
	},
	"exports": {
		"./*": {
			"import": "./.tsc/dist/*.js",
			"types": "./.tsc/dist/*.d.ts"
		}
	},
	"files": [
		".tsc/dist/**/*.js",
		".tsc/dist/**/*.ts",
		".tsc/dist/**/*.map"
	],
	"scripts": {
		"clean": "rm -rf .prettier .package .tsc",
		"format": "prettier --check --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"format:fix": "prettier --write --cache --cache-location='.prettier/cache' --cache-strategy content .",
		"assets": "tsc --build",
		"assets:watch": "tsc --build --watch",
		"check:tsc": "tsc --build --noEmit",
		"lint:eslint": "eslint --max-warnings 0 .",
		"lint:eslint:fix": "eslint --fix .",
		"inspect:eslint": "eslint --inspect-config ."
	},
	"peerDependencies": {},
	"dependencies": {
		"yo": "5.1.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@duncan3142/prettier-config": "^0.1.3",
		"@duncan3142/tsc-config": "^0.1.5",
		"@duncan3142/eslint-config": "^0.1.10",
		"@eslint/config-inspector": "^1.0.2",
		"eslint": "^9.23.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"volta": {
		"node": "22.13.1",
		"npm": "11.1.0"
	}
}
